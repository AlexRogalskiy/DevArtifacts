      .Gallery-left
        .Gallery-image
        .Gallery-image
      .Gallery-image.Gallery-image--primary(style="background-image: url(https://robohash.org/"+i+")")
      
    - for (var j=1; j<=4; j++)
      .Gallery-images
        .Gallery-image
