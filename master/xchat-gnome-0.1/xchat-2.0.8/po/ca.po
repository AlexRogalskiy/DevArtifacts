"\n"
"  "

#: src/common/outbound.c:1725
#, c-format
msgid "Unknown arg '%s' ignored."
msgstr "L'argument desconegut '%s' serà ignorat."

#: src/common/outbound.c:2450
msgid "No such plugin found.\n"
msgstr "No s'ha trobat el connector.\n"

#: src/common/outbound.c:2455 src/fe-gtk/plugingui.c:186
msgid "That plugin is refusing to unload.\n"
msgstr "Aquest connector ha refusat de descarregar-se.\n"

#: src/common/outbound.c:2600
msgid "ADDBUTTON <name> <action>, adds a button under the user-list"
msgstr "ADDBUTTON <nom> <acció>, afegeix un botó a sota de la llista d'usuaris"

#: src/common/outbound.c:2602
msgid "ALLCHAN <cmd>, sends a command to all channels you're in"
msgstr "ALLCHAN <cmd>, envia un comandament a tots els canals on estigueu"

#: src/common/outbound.c:2604
msgid "ALLSERV <cmd>, sends a command to all servers you're in"
msgstr "ALLSERV <cmd>, envia un comandament a tots els servidors on estigueu"

#: src/common/outbound.c:2605
msgid "AWAY [<reason>], sets you away"
msgstr "AWAY [<motiu>], estableix el motiu de la vostra absència"

#: src/common/outbound.c:2607
msgid ""
"BAN <mask> [<bantype>], bans everyone matching the mask from the current "
"channel. If they are already on the channel this doesn't kick them (needs "
"chanop)"
msgstr ""
"BAN <màscara> [<tipus_de_veto>], no permetre l'entrada a ningú (en el canal "
"actual) que coincideixi amb la màscara. Si ja estan en el canal això no els "
"farà fora (necessitareu ser operador del canal)"

#: src/common/outbound.c:2609
msgid "CLEAR, Clears the current text window"
msgstr "CLEAR, Neteja el text de la finestra actual"

#: src/common/outbound.c:2610
msgid "CLOSE, Closes the current window/tab"
msgstr "CLOSE, Tanca l'actual finestra o pestanya"

#: src/common/outbound.c:2613
msgid "COUNTRY <code>, finds a country code, eg: au = australia"
msgstr "COUNTRY <codi>, cerca al país d'un codi, p.ex: au = Austràlia"

#: src/common/outbound.c:2615
msgid ""
"CTCP <nick> <message>, send the CTCP message to nick, common messages are "
"VERSION and USERINFO"
msgstr ""
"CTCP <sobrenom> <missatge>, envia el missatge CTCP al sobrenom, els "
"missatges comuns són VERSION i USERINFO"

#: src/common/outbound.c:2617
msgid "CYCLE, parts current channel and immediately rejoins"
msgstr "CYCLE, surt i retorna de seguida al canal actual"

#: src/common/outbound.c:2619
msgid ""
"\n"
"DCC GET <nick>                     - accept an offered file\n"
"DCC SEND [-maxcps=#] <nick> [file] - send a file to someone\n"
"DCC LIST                           - show DCC list\n"
"DCC CHAT <nick>                    - offer DCC CHAT to someone\n"
"DCC CLOSE <type> <nick> <file>         example:\n"
"         /dcc close send johnsmith file.tar.gz"
msgstr ""
"\n"
"DCC GET <sobrenom>                       - rep un fitxer un fitxer ofert\n"
"DCC SEND [-maxcps=#] <sobrenom> [fitxer] - envia un fitxer a algú\n"
"DCC LIST                                 - mostra la llista DCC\n"
"DCC CHAT <sobrenom>                      - proposa un CHAT DCC a algú\n"
"DCC CLOSE <tipus> <sobrenom> <fitxer>      exemple:\n"
"         /dcc close send albanoguer fitxer.tar.gz"

#: src/common/outbound.c:2629
msgid ""
"DEHOP <nick>, removes chanhalf-op status from the nick on the current "
"channel (needs chanop)"
msgstr ""
"DEHOP <sobrenom>, treure l'estatus de semi-operador del canal al sobrenom "
"especificat del canal actual (necessitareu ser operador del canal)"

#: src/common/outbound.c:2631
msgid "DELBUTTON <name>, deletes a button from under the user-list"
msgstr "DELBUTTON <nom>, esborra un botó de sota la llista d'usuaris"

#: src/common/outbound.c:2633
msgid ""
"DEOP <nick>, removes chanop status from the nick on the current channel "
"(needs chanop)"
msgstr ""
"DEOP <sobrenom>, treure l'estatus d'operador del canal al sobrenom "
"especificat del canal actual (necessitareu ser operador del canal)"

#: src/common/outbound.c:2635
msgid ""
"DEVOICE <nick>, removes voice status from the nick on the current channel "
"(needs chanop)"
msgstr ""
"DEVOICE <sobrenom>, treure l'estatus de veu al sobrenom especificat del "
"canal actual (necessitareu ser operador del canal)"

#: src/common/outbound.c:2636
msgid "DISCON, Disconnects from server"
msgstr "DISCON, Desconnecta del servidor"

#: src/common/outbound.c:2637
msgid "DNS <nick|host|ip>, Finds a users IP number"
msgstr "DNS <sobrenom|màquina|ip>, Cerca el número IP d'un usuari"

#: src/common/outbound.c:2638
msgid "ECHO <text>, Prints text locally"
msgstr "ECHO <text>, Imprimeix el text localment"

#: src/common/outbound.c:2641
msgid ""
"EXEC [-o] <command>, runs the command. If -o flag is used then output is "
"sent to current channel, else is printed to current text box"
msgstr ""
"EXEC [-o] <comandament>, executa el comandament. Si useu l'argument -o "
"s'enviarà l'eixida cap al canal actual, sinó serà imprimida en la caixa de "
"text actual"

#: src/common/outbound.c:2643
msgid "EXECCONT, sends the process SIGCONT"
msgstr "EXECCONT, envia el procés SIGCONT"

#: src/common/outbound.c:2646
msgid ""
"EXECKILL [-9], kills a running exec in the current session. If -9 is given "
"the process is SIGKILL'ed"
msgstr ""
"EXECKILL [-9], mata a un procés que s'estigui executant en l'actual sessió. "
"Si s'empra -9 el procés serà SIGKILL (mort al moment)"

#: src/common/outbound.c:2648
msgid "EXECSTOP, sends the process SIGSTOP"
msgstr "EXECSTOP, envia el procés SIGSTOP"

#: src/common/outbound.c:2649
msgid "EXECWRITE, sends data to the processes stdin"
msgstr "EXECWRITE, envia dades als processos en stdin (entrada estàndard)"

#: src/common/outbound.c:2653
msgid "FLUSHQ, flushes the current server's send queue"
msgstr "FLUSHQ, neteja la cua d'enviaments del servidor actual"

#: src/common/outbound.c:2655
msgid "GATE <host> [<port>], proxies through a host, port defaults to 23"
msgstr ""
"GATE <màquina> [<port>], servidor intermedi a través d'una màquina, port per "
"omissió el 23"

#: src/common/outbound.c:2661
msgid "HOP <nick>, gives chanhalf-op status to the nick (needs chanop)"
msgstr ""
"HOP <sobrenom>, dona l'estatus d'operador del canal al sobrenom especificat "
"(necessitareu ser operador del canal)"

#: src/common/outbound.c:2663
msgid ""
"IGNORE <mask> <types..> <options..>\n"
"    mask - host mask to ignore, eg: *!*@*.aol.com\n"
"    types - types of data to ignore, one or all of:\n"
"            PRIV, CHAN, NOTI, CTCP, DCC, INVI, ALL\n"
"    options - NOSAVE, QUIET"
msgstr ""
"IGNORE <màscara> <tipus..> <opcions..>\n"
"    màscara - màscara de la màquina a ignorar, p.ex: *!*@*.aol.com\n"
"    tipus   - tipus de dades a ignorar, una de les següents o totes (all):\n"
"              PRIV, CHAN, NOTI, CTCP, DCC, INVI, ALL\n"
"    opcions - NOSAVE, QUIET"

#: src/common/outbound.c:2670
msgid ""
"INVITE <nick> [<channel>], invites someone to a channel, by default the "
"current channel (needs chanop)"
msgstr ""
"INVITE <sobrenom> [<canal>], invita a algú a un canal, per omissió al canal "
"actual (necessitareu ser operador del canal)"

#: src/common/outbound.c:2671
msgid "JOIN <channel>, joins the channel"
msgstr "JOIN <canal>, entra en el canal"

#: src/common/outbound.c:2673
msgid "KICK <nick>, kicks the nick from the current channel (needs chanop)"
msgstr ""
"KICK <sobrenom>, fa fora al sobrenom del canal actual (necessitareu ser "
"operador del canal)"

#: src/common/outbound.c:2675
msgid ""
"KICKBAN <nick>, bans then kicks the nick from the current channel (needs "
"chanop)"
msgstr ""
"KICKBAN <sobrenom>, veta al sobrenom després de fer-lo fora del canal actual "
"(necessitareu ser operador del canal)"

#: src/common/outbound.c:2678
msgid "LAGCHECK, forces a new lag check"
msgstr "LAGCHECK, força una nova comprovació de la demora"

#: src/common/outbound.c:2680
msgid "LASTLOG <string>, searches for a string in the buffer"
msgstr "LASTLOG <cadena>, cerca una cadena de caràcters en el cau temporal"

#: src/common/outbound.c:2682
msgid "LOAD <file>, loads a plugin or script"
msgstr "LOAD <fitxer>, carrega un connector o script"

#: src/common/outbound.c:2685
msgid ""
"MDEHOP, Mass deop's all chanhalf-ops in the current channel (needs chanop)"
msgstr ""
"MDEHOP, Treure l'estatus a tots els semi-operadors del canal en el canal "
"actual (necessitareu ser operador del canal)"

#: src/common/outbound.c:2687
msgid "MDEOP, Mass deop's all chanops in the current channel (needs chanop)"
msgstr ""
"MDEOP, Treure l'estatus a tots els operadors del canal en el canal actual "
"(necessitareu ser operador del canal)"

#: src/common/outbound.c:2689
msgid ""
"ME <action>, sends the action to the current channel (actions are written in "
"the 3rd person, like /me jumps)"
msgstr ""
"ME <acció>, envia l'acció al canal actual (les accions s'escriuen en tercera "
"persona, com ara /me jumps)"

#: src/common/outbound.c:2691
msgid ""
"MKICK, Mass kicks everyone except you in the current channel (needs chanop)"
msgstr ""
"MKICK, Fa fora a tothom excepte a tu en el canal actual (necessitareu ser "
"operador del canal)"

#: src/common/outbound.c:2693
msgid "MOP, Mass op's all users in the current channel (needs chanop)"
msgstr ""
"MOP, Dona l'estatus d'operador a tots els usuaris en el canal actual "
"(necessitareu ser operador del canal)"

#: src/common/outbound.c:2694
msgid "MSG <nick> <message>, sends a private message"
msgstr "MSG <sobrenom> <missatge>, envia un missatge privat"

#: src/common/outbound.c:2697
msgid "NAMES, Lists the nicks on the current channel"
msgstr "NAMES, Llista els sobrenoms en el canal actual"

#: src/common/outbound.c:2699
msgid "NCTCP <nick> <message>, Sends a CTCP notice"
msgstr "NCTCP <sobrenom> <missatge>, Envia un notícia CTCP"

#: src/common/outbound.c:2700
msgid "NEWSERVER <hostname> [<port>]"
msgstr "NEWSERVER <nom_màquina> [<port>]"

#: src/common/outbound.c:2701
msgid "NICK <nickname>, sets your nick"
msgstr "NICK <sobrenom>, estableix el vostre sobrenom"

#: src/common/outbound.c:2704
msgid ""
"NOTICE <nick/channel> <message>, sends a notice. Notices are a type of "
"message that should be auto reacted to"
msgstr ""
"NOTICE <sobrenom/canal> <missatge>, envia una notícia. Les notícies són un "
"tipus de missatge que hauria de ser publicat automàticament"

#: src/common/outbound.c:2706
msgid "NOTIFY [<nick>], lists your notify list or adds someone to it"
msgstr ""
"NOTIFY [<sobrenom>], mostra la vostra llista de notificacions o hi afegeix a "
"algú"

#: src/common/outbound.c:2708
msgid "OP <nick>, gives chanop status to the nick (needs chanop)"
msgstr ""
"OP <sobrenom>, dona l'estatus d'operador del canal al sobrenom (necessitareu "
"ser operador del canal)"

#: src/common/outbound.c:2710
msgid ""
"PART [<channel>] [<reason>], leaves the channel, by default the current one"
msgstr "PART [<canal>] [<motiu>], abandona el canal, por defecte serà l'actual"

#: src/common/outbound.c:2712
msgid "PING <nick | channel>, CTCP pings nick or channel"
msgstr "PING <sobrenom | canal>, pings CTCP al sobrenom o al canal"

#: src/common/outbound.c:2714
msgid "QUERY <nick>, opens up a new privmsg window to someone"
msgstr ""
"QUERY <sobrenom>, obre una nova finestra de missatges privada per a algú"

#: src/common/outbound.c:2716
msgid "QUIT [<reason>], disconnects from the current server"
msgstr "QUIT [<motiu>], desconnecta del servidor actual"

#: src/common/outbound.c:2718
msgid "QUOTE <text>, sends the text in raw form to the server"
msgstr "QUOTE <text>, envia el text en format pla cap al servidor"

#: src/common/outbound.c:2721
msgid ""
"RECONNECT [-ssl] [<host>] [<port>] [<password>], Can be called just as /"
"RECONNECT to reconnect to the current server or with /RECONNECT ALL to "
"reconnect to all the open servers"
msgstr ""
"RECONENCT [-ssl] [<sistema>] [<port>] [<contrasenya>], Pot invocar-se sols "
"amb /RECONNECT per així reconnectar-se al servidor actual o amb /RECONNECT "
"ALL per a reconnectar-se a tots els servidors oberts"

#: src/common/outbound.c:2724
msgid ""
"RECONNECT [<host>] [<port>] [<password>], Can be called just as /RECONNECT "
"to reconnect to the current server or with /RECONNECT ALL to reconnect to "
"all the open servers"
msgstr ""
"RECONENCT [<sistema>] [<port>] [<contrasenya>], Pot invocar-se sols amb /"
"RECONNECT per així reconnectar-se al servidor actual o amb /RECONNECT ALL "
"per a reconnectar-se a tots els servidors oberts"

#: src/common/outbound.c:2726
msgid ""
"RECV <text>, send raw data to xchat, as if it was received from the irc "
"server"
msgstr ""
"RECV <text>, envia dades en format pla cap a l'xchat, com si aquestes foren "
"rebudes des del servidor IRC"

#: src/common/outbound.c:2729
msgid "SAY <text>, sends the text to the object in the current window"
msgstr "SAY <text>, envia el text cap a l'objectiu en la finestra actual"

#: src/common/outbound.c:2732
msgid "SERVCHAN [-ssl] <host> <port> <channel>, connects and joins a channel"
msgstr "SERVCHAN [-ssl] <màquina> <port> <canal>, connecta i entra en un canal"

#: src/common/outbound.c:2735
msgid "SERVCHAN <host> <port> <channel>, connects and joins a channel"
msgstr "SERVCHAN <màquina> <port> <canal>, connecta i entra en un canal"

#: src/common/outbound.c:2739
msgid ""
"SERVER [-ssl] <host> [<port>] [<password>], connects to a server, the "
"default port is 6667 for normal connections, and 9999 for ssl connections"
msgstr ""
"SERVER [-ssl] <màquina> [<port>] [<contrasenya>], connecta a un servidor, el "
"port per omissió per a connexions normals és el 6667, i el 9999 per a "
"connexions ssl"

#: src/common/outbound.c:2742
msgid ""
"SERVER <host> [<port>] [<password>], connects to a server, the default port "
"is 6667"
msgstr ""
"SERVER <màquina> [<port>] [<contrasenya>], connecta a un servidor, el port "
"per omissió és el 6667"

#: src/common/outbound.c:2744
msgid "SET <variable> [<value>]"
msgstr "SET <variable> [<valor>]"

#: src/common/outbound.c:2747
msgid ""
"TOPIC [<topic>], sets the topic if one is given, else shows the current topic"
msgstr ""
"TOPIC [<tòpic>], estableix el tòpic si se'n dona un, d'altra manera es "
"mostra el tòpic actual"

#: src/common/outbound.c:2749
msgid "UNBAN <mask> [<mask>...], unbans the specified masks."
msgstr ""
"UNBAN <màscara> [<màscara>...], reincorpora les màscares especificades."

#: src/common/outbound.c:2750
msgid "UNIGNORE <mask> [QUIET]"
msgstr "UNIGNORE <màscara> [QUIET]"

#: src/common/outbound.c:2751
msgid "UNLOAD <name>, unloads a plugin or script"
msgstr "LOAD <fitxer>, carrega un connector o script"

#: src/common/outbound.c:2754
msgid "VOICE <nick>, gives voice status to someone (needs chanop)"
msgstr ""
"VOICE <sobrenom>, dona l'estatus de veu a algú (necessitareu ser operador "
"del canal)"

#: src/common/outbound.c:2756
msgid "WALLCHAN <message>, writes the message to all channels"
msgstr "WALLCHAN <missatge>, escriu el missatge per a tots els canals"

#: src/common/outbound.c:2758
msgid ""
"WALLCHOP <message>, sends the message to all chanops on the current channel"
msgstr ""
"WALLCHOP <missatge>, envia el missatge a tots els operadors del canal en el "
"canal actual"

#: src/common/outbound.c:2791
#, c-format
msgid "Usage: %s\n"
msgstr "Usant: %s\n"

#: src/common/outbound.c:2796
msgid ""
"\n"
"No help available on that command.\n"
msgstr ""
"\n"
"No hi ha cap ajuda disponible per aquest comandament.\n"

#: src/common/outbound.c:2802
msgid "No such command.\n"
msgstr "No existeix tal comandament.\n"

#: src/common/outbound.c:3126
msgid "Bad arguments for user command.\n"
msgstr "Arguments erronis per al comandament emprat.\n"

#: src/common/outbound.c:3290
msgid "Too many recursive usercommands, aborting."
msgstr "Masses comandaments d'usuari recursius, avortant."

#: src/common/outbound.c:3363
msgid "Unknown Command. Try /help\n"
msgstr "Comandament desconegut. Proveu amb /help\n"

#: src/common/plugin.c:363 src/common/plugin.c:393
msgid "No xchat_plugin_init symbol; is this really an xchat plugin?"
msgstr ""
"No hi ha cap símbol xchat_plugin_init; de veres és un connector per a "
"l'xchat?"

#: src/common/server.c:909
#, c-format
msgid ""
"Cannot resolve hostname %s\n"
"Check your IP Settings!\n"
msgstr ""
"No s'ha pogut resoldre el nom de la màquina %s\n"
"Comproveu el vostre arranjament IP!\n"

#: src/common/server.c:914
msgid "Proxy traversal failed.\n"
msgstr "Ha fallat el servidor intermediari travesser.\n"

#: src/common/servlist.c:657
#, c-format
msgid "Cycling to next server in %s...\n"
msgstr ""

#: src/common/servlist.c:1002
#, c-format
msgid ""
"Warning: \"%s\" character set is unknown. No conversion will be applied for "
"network %s."
msgstr ""

#: src/common/textevents.h:6
msgid "-%C10-%C11-%O$t$1 added to notify list."
msgstr "-%C10-%C11-%O$t$1 ha estat afegit a la llista de notificacions."

#: src/common/textevents.h:9
msgid "-%C10-%C11-%O$t$1 Banlist: %C3$4 %C4$2 %C5$3%O"
msgstr "-%C10-%C11-%O$t$1 Llista de veto: %C3$4 %C4$2 %C5$3%O"

#: src/common/textevents.h:12
msgid "-%C10-%C11-%O$tCannot join%C11 %B$1 %O(You are banned)."
msgstr "-%C10-%C11-%O$tNo es pot entrar%C11 %B$1 %O(Su's ha vetat l'accés)."

#: src/common/textevents.h:15
