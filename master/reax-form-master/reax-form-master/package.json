{
    "name": "reax-form",
    "version": "1.0.33",
    "license": "MIT",
    "author": "Damiano Barbati <damiano.barbati@gmail.com> (http://github.com/damianobarbati)",
    "repository": "https://github.com/damianobarbati/reax-form",
    "main": "src/index.js",
    "module": "src/index.js",
    "jsnext:main": "src/index.js",
    "scripts": {
        "jslint": "eslint **/*.js --ignore-path .gitignore --ignore-pattern **/*.min.js ."
    },
    "eslintConfig": {
        "parser": "babel-eslint",
        "parserOptions": {
            "ecmaVersion": 2017,
            "sourceType": "module",
            "impliedStrict": true,
            "ecmaFeatures": {
                "jsx": true,
                "impliedStrict": true,
                "globalReturn": false,
                "experimentalObjectRestSpread": true
            }
        },
        "env": {
            "browser": true,
            "node": true,
            "jest": true,
            "es6": true
        },
        "plugins": [
            "import",
            "react",
            "jest"
        ],
        "extends": [
            "eslint:recommended",
            "plugin:react/recommended",
            "plugin:jest/recommended"
        ],
        "rules": {
            "no-console": "off",
            "no-unsafe-finally": "off",
            "no-unused-vars": "off",
            "react/no-unescaped-entities": "off"
        }
    },
    "peerDependencies": {
        "lodash-es": "^4.17.4",
        "material-ui": "^1.0.0-beta.25",
        "moment": "^2.18.1",
        "moment-timezone": "^0.5.13",
        "muicons": "^1.0.11",
        "prop-types": "^15.6.0",
        "react": "^16.2.0",
        "react-dom": "^16.2.0"
    },
    "dependencies": {
        "autosize": "^4.0.0",
        "babel-eslint": "8.1.2",
        "classnames": "2.2.5",
        "deep-diff": "^0.3.8",
        "eslint": "4.14.0",
        "eslint-loader": "1.9.0",
        "eslint-plugin-import": "2.8.0",
        "eslint-plugin-jest": "21.5.0",
        "eslint-plugin-react": "7.5.1",
        "keycode": "2.1.9",
        "mime-types": "2.1.17",
        "react-markdown": "^3.1.4",
        "remark-breaks": "^1.0.0",
        "sha512": "^0.0.1",
        "trigger-upload": "1.0.3"
    },
    "devDependencies": {}
}
