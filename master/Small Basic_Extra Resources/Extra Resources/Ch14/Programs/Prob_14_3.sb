' Finds the max number entered by the user
' A value of 0 ends the loop

number = TextWindow.ReadNumber()
max = number
While (number <> 0)
  number = TextWindow.ReadNumber()
  If (number > max) Then
    max = number
  EndIf
EndWhile
TextWindow.WriteLine("max is " + max)