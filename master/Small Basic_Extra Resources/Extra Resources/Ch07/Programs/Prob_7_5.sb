' Computes the period of a pendulum

L = 96                            ' The pendulum's length in feet

T = 1.1 * Math.SquareRoot(L)

T = Math.Round(T * 1000) / 1000   ' Rounds to three decimal places

' Displays the result
TextWindow.WriteLine("The period is: " + T + " seconds")
TextWindow.WriteLine("")