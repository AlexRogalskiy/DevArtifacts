' Displays all the factors of an integer number, N, entered by a user

TextWindow.Write("Enter N: ")
N = TextWindow.ReadNumber()

TextWindow.WriteLine("Factors of " + N + ":")
For I = 1 To N
  If (Math.Remainder(N, I) = 0) Then
    TextWindow.WriteLine(I)
  EndIf
EndFor