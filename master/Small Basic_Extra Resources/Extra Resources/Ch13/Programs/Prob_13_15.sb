' Shows all the possible background colors for the graphics window

For R = 0 to 255
  For G = 0 to 255
    For B = 0 to 255
      GraphicsWindow.Title = R + "," + G + "," + B
      GraphicsWindow.BackgroundColor = GraphicsWindow.GetColorFromRGB(R,G,B)
      Program.Delay(10)      
    EndFor
  EndFor
EndFor