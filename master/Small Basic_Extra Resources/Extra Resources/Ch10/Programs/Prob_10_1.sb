' Converts quarts to liters

TextWindow.Write("How many quarts do you have? ")
quart = TextWindow.ReadNumber()

QuartToLiter()

TextWindow.WriteLine(quart + " quarts = " + liter + " liters")

Sub QuartToLiter
  liter = 0.946 * quart
EndSub