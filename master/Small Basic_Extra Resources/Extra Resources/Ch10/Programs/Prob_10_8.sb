' Converts feet per second to miles per hour

TextWindow.Write("Enter your speed (in feet per second): ")
fps = TextWindow.Read()

SpeedConvert()

TextWindow.WriteLine("That is " + mph + " miles per hour.")
TextWindow.WriteLine("")

' 1 foot per second is 0.681818 miles per hour
Sub SpeedConvert
  mph = fps * 0.681818
  mph = Math.Round(mph * 100) / 100
EndSub