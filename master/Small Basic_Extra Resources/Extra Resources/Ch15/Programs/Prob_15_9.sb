' Creates a symmetric array

For N = 1 To 5
  TextWindow.Write("Number " + N + ": ")
  num = TextWindow.ReadNumber()
  
  arr[N] = num
  arr[11 - N] = num  
EndFor

' Displays the result
TextWindow.Write("Your array is: ")
For N = 1 To 10
  TextWindow.Write(arr[N] + "  ")
EndFor
TextWindow.WriteLine("")  
TextWindow.WriteLine("")