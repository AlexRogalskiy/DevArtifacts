' Displays the contents in a matrix 

For I = 1 To 4
  For J = 1 To 4
    mat[I][J] = I * J
  EndFor
EndFor

' Displays the matrix
For I = 1 To 4
  For J = 1 To 4
    TextWindow.CursorLeft = (J - 1) * 4
    TextWindow.Write(mat[I][J])
  EndFor
  TextWindow.WriteLine("")
EndFor
TextWindow.WriteLine("")