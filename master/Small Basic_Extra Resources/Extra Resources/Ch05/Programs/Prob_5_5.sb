' Draws a square octagon pattern

GraphicsWindow.Title = "Square Octagon"
Turtle.Speed = 10
Turtle.X = 40
Turtle.Y = 100

For I = 1 To 8    ' Rotates the pattern eight times
  
  'Draws the square
  For K = 1 To 4
    Turtle.Move(30)
    Turtle.Turn(-90)
  EndFor
  Turtle.Move(30)
  
  Turtle.Turn(45)

EndFor

Turtle.Hide()