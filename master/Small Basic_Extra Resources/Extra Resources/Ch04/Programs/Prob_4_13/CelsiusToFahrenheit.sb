degC = 25
degF = (degC * (9 / 5)) + 32
degF = Math.Round(degF)        ' Rounds to the nearest integer

' Displays the result
TextWindow.WriteLine(degC + " °C = " + degF + " °F")
TextWindow.WriteLine("")