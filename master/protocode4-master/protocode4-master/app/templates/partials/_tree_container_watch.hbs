{{#each uiWatchControls}}
    {{#if uiWatchControls}}
        <li><span><i class="fa fa-plus-square-o"></i> {{#link-to 'dispatchUiWatchControl'
                                                                 this}}{{name}}{{/link-to}}</span>
            <ul>
                {{partial "partials/tree_container_watch"}}
            </ul>
        </li>
    {{else}}
        <li>{{#link-to 'dispatchUiWatchControl' this}}{{name}}{{/link-to}}</li>
    {{/if}}
{{/each}}
