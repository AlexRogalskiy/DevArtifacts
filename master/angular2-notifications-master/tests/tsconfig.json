{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "removeComments": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "declaration": true,
    "types": [
      "jasmine",
      "node"
    ],
    "lib": ["es2015", "dom"]
  },
  "exclude": [
    "node_modules"
  ]
}
