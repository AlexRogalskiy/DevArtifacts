{
    "root": true,
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "jquery": true
    },
    "rules": {
        "import/no-unresolved": ["error", { "ignore": ["jquery"] }],
        "import/extensions": ["error", { "jquery": "never" }]
    },
    "extends": "airbnb-base"
}
