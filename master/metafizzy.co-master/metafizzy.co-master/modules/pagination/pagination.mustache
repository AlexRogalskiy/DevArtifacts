<nav class="pagination">
  {{#if previousPage}}
    <a class="pagination__previous" href="{{previousPage.url}}">Previous &middot; {{previousPage.index}}</a>
  {{/if}}
  <span class="pagination__current">Page {{currentPage}} of {{totalPages}}</span>
  {{#if nextPage}}
    <a class="pagination__next" href="/blog/page{{nextPage}}">Next &middot; {{nextPage}}</a>
  {{/if}}
</nav>
