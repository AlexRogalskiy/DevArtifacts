/**!
 * See http://www.jshint.com/docs/options/ for all available options.
 */
{
  /**
   * Enforcing options
   * See http://www.jshint.com/docs/options/#enforcing-options
   */
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "immed": true,
  "latedef": true,
  "newcap": false,
  "noarg": true,
  "strict": true,
  "trailing": true,
  "undef": true,
  "unused": true,

  /**
   * Relaxing options
   * See http://www.jshint.com/docs/options/#relaxing-options
   */
  "eqnull": true,
  "esnext": true,
  "smarttabs": true,

  /**
   * Environments
   * See http://www.jshint.com/docs/options/#environments
   */
  "browser": true,
  "jquery": true,
  "node": true,

  /**
   * Globals
   * See http://www.jshint.com/docs/
   */
  "globals": {

    // AMD & CommomJS
    "define": false,
    "module": false,
    "require": false,

    // Jasmine
    "jasmine": false,
    "afterEach": false,
    "beforeEach": false,
    "describe": false,
    "expect": false,
    "it": false,
    "spyOn": false
  }
}
