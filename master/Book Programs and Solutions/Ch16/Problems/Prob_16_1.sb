' Keeps track of your friends' phone numbers

phone["Yoda"] = "555-1138"
phone["Amidala"] = "555-0599"
phone["Rey"] = "555-1215"
phone["R2-D2"] = "555-0577"

TextWindow.Write("Enter the name of your friend: ")
name = TextWindow.Read()
If (phone[name] = "") Then
  TextWindow.WriteLine("Sorry. " + name + " is not in your phone book.")
Else
  TextWindow.WriteLine(name + "'s phone number is: " + phone[name])
EndIf