' TryItOut_16_1.sb
day["Sunday"] = "Dimanche"
day["Monday"] = "Lundi"
day["Tuesday"] = "Mardi"
day["Wednesday"] = "Mercredi"
day["Thursday"] = "Jeudi"
day["Friday"] = "Vendredi"
day["Saturday"] = "Samedi"

TextWindow.Write("Enter the name of a day: ")
name = TextWindow.Read()

If (day[name] = "") Then
  TextWindow.WriteLine("Sorry. You entered an invalid day of the week.")
Else
  TextWindow.WriteLine(name + " in French is " + day[name])
EndIf  