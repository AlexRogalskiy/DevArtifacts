TextWindow.WriteLine("Hello, Helen!")
TextWindow.Write("Please enter the total purchase amount: ")
totPrice = TextWindow.ReadNumber()

' Add 6% sales tax
totWithTax = totPrice * 1.06

' Round to nearest hundredth
charge = Math.Round(totWithTax * 100) / 100

TextWindow.WriteLine("Helen, please charge the customer $" + charge)
TextWindow.WriteLine("")