' TryItOut_9_1.sb

TextWindow.Write("Enter the score: ")
score = TextWindow.ReadNumber()
If (score < 60) Then
  grade = "F"
ElseIf (score < 70) Then
  grade = "D"
ElseIf (score < 80) Then
  grade = "C"
ElseIf (score < 90) Then
  grade = "B"
Else
  grade = "A"
EndIf
TextWindow.WriteLine("The grade is " + grade + ".")