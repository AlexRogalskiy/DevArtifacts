' SquareTable.sb
TextWindow.Title = "Table of Squares"
TextWindow.WriteLine(" Number     Square")
TextWindow.WriteLine("========   =========")

For N = 1 To 5
  TextWindow.CursorLeft = 3   ' Moves to middle of col 1
  TextWindow.Write(N)         ' Writes the number
  TextWindow.CursorLeft = 14  ' Moves to next column
  TextWindow.WriteLine(N * N) ' Writes its square
EndFor