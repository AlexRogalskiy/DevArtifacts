' CoinsAdder.sb
TextWindow.WriteLine("Quarters    Dimes    Nickels")
TextWindow.WriteLine("--------    -----    -------")

For Q = 0 TO 2       ' Quarters
  For D = 0 TO 5     ' Dimes
    For N = 0 TO 10  ' Nickels
      If (Q * 25 + D * 10 + N * 5 = 50) Then
        TextWindow.Write(Q)
        TextWindow.CursorLeft = 13
        TextWindow.Write(D)
        TextWindow.CursorLeft = 24
        TextWindow.WriteLine(N)
      EndIf
    EndFor
  EndFor
EndFor        