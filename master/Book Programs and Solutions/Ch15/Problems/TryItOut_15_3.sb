' Fills an array called burps with five random numbers between 80 and 100
For N = 1 To 5
  burps[N] = 79 + Math.GetRandomNumber(21)
EndFor

' 1 - Display in one statement
TextWindow.WriteLine(burps)
TextWindow.WriteLine("")

' 2 - Display in a for loop
For N = 1 To 5
  TextWindow.WriteLine("burps[" + N + "] = " + burps[N])
EndFor