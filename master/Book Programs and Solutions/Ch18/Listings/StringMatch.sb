' StringMatch.sb
While ("True")
  TextWindow.Write("Who's your favorite Shrek character? ")
  name = Text.ConvertToUpperCase(TextWindow.Read())
  If (name = "DONKEY") Then
    TextWindow.WriteLine("You won 200 ogre points!")
  Else
    TextWindow.WriteLine("You won 100 ogre points!")
  EndIf
EndWhile    