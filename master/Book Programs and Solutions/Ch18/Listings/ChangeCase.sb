' ChangeCase.sb
var1 = "Ewok"
lwrCase = Text.ConvertToLowerCase(var1)   ' lwrCase = "ewok"
TextWindow.WriteLine(lwrCase)             ' Dispalys: ewok
TextWindow.WriteLine(var1)                ' Displays: Ewok
uprCase = Text.ConvertToUpperCase(var1)   ' uprCase = "EWOK"
TextWindow.WriteLine(uprCase)             ' Displays: EWOK
TextWindow.WriteLine(var1)                ' Displays: Ewok