' Prob_5_1.sb
' Draws a flower by rotating a circle

GraphicsWindow.Title = "Circular Flower"
Turtle.Speed = 10
Turtle.X = 80
Turtle.Y = 80

For I = 1 To 12     ' Rotates 12 time
  
  For K = 1 To 36   ' Draws a circle
    Turtle.Move(6)
    Turtle.Turn(10)
  EndFor
  
  Turtle.Turn(30)
EndFor

Turtle.Hide()