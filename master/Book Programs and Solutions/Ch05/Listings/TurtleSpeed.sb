' TurtleSpeed.sb
Turtle.Show()
Turtle.Speed = 2    ' Sets the initial speed to 2
Turtle.Move(100)    ' Moves the turtle forward 100 pixels
Turtle.Speed = 5    ' Changes the speed to 5
Turtle.TurnRight()  ' Turns the turtle to its right
Turtle.Move(100)
Turtle.Speed = 9    ' Changes the speed to 9
Turtle.TurnRight()
Turtle.Move(100)