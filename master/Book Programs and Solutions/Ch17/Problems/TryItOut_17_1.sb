' Fills the matrix by columns

' For each cloumn, fills the values in the three rows
For c = 1 To 4    ' 4 columns
  For r = 1 To 3  ' 3 rows 
    mat[r][c] = Math.GetRandomNumber(9)
  EndFor
EndFor

' Displays the matrix to see its contents
For r = 1 To 3    ' 3 rows
  For c = 1 To 4  ' 4 columns
    TextWindow.Write(mat[r][c] + " ")
  EndFor
  TextWindow.WriteLine("")
EndFor