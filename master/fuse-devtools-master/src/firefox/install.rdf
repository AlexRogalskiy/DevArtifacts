<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
    <Description about="urn:mozilla:install-manifest">
        <em:id><%= browser.firefox.extensionId %></em:id>
        <em:version><%= pkg.version %></em:version>
        <em:name><%= i18n.extensionName %></em:name>
        <em:description><%= i18n.extensionDescription %></em:description>
        <em:creator><%= pkg.author.name %></em:creator>
        <em:homepageURL><%= pkg.homepage %></em:homepageURL>
        <em:iconURL>chrome://<%= pkg.name %>/skin/img/icon-48.png</em:iconURL>
        <em:targetApplication>
            <Description>
                <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                <em:minVersion><%= browser.firefox.minVersion %></em:minVersion>
                <em:maxVersion><%= browser.firefox.maxVersion %></em:maxVersion>
            </Description>
        </em:targetApplication>
        <em:type>2</em:type>
        <em:bootstrap>true</em:bootstrap>
    </Description>
</RDF>